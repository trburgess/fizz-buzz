{
    "name": "trburgess/fizz-buzz",
    "type": "library",
    "description": "Fizz Buzz test implementation.",
    "keywords": ["fizz", "buzz"],
    "homepage": "https://github.com/trburgess/fizz-buzz",
    "license": "MIT",
    "authors": [
        {
            "name": "Tom Burgess",
            "email": "tom.burgess@tburgess.net"
        }
    ],
    "require": {
        "php": ">=5.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8|^5.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4" : {
            "FizzBuzz\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4" : {
            "FizzBuzz\\Tests\\" : "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
